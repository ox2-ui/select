{
  "name": "@ox2/select",
  "version": "5.1.1",
  "description": "Wrapper components of react-select package.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ox2/select"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files",
    "build-storybook": "build-storybook -s ./public",
    "build:babel": "babel ./src --ignore *.story.js,*.test.js --out-dir ./build",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "clean:build": "rimraf build",
    "docs": "styleguidist server",
    "docs:build": "styleguidist build && npm run docs:copy-to-public",
    "docs:copy-to-public": "bash ./scripts/copy_docs.sh",
    "lint": "eslint src scripts && echo \"eslint: NO LINT ERRORS\"; exit 0",
    "prebuild": "npm run clean:build && npm run lint && npm run test",
    "prettier": "prettier --print-width 60 --single-quote --trailing-comma all --write \"{src,.storybook,scripts}/**/*.js\"",
    "storybook": "start-storybook -p 55011 -s ./public",
    "test": "jest",
    "test:update": "yarn test -- -u"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-options": "^1.0.2",
    "@ox2/button-css": "^1.2.0",
    "@ox2/css-font-roboto": "^1.0.0",
    "@ox2/css-font-roboto-condensed": "^3.0.0",
    "@ox2/css-icons-hq-modules": "^0.0.1",
    "@ox2/css-icons-linear": "^0.0.1",
    "@ox2/css-icons-material": "^0.0.1",
    "@ox2/css-icons-moon": "^0.0.1",
    "@ox2/css-icons-ui": "^0.1.2",
    "@ox2/iframe": "^0.0.3",
    "@ox2/storybook-css-layout": "^1.0.1",
    "@ox2/theming": "^2.0.2",
    "@ox2/ycss": "^1.5.0",
    "@tapfuse/theme-spark": "^1.1.0",
    "@tapfuse/ycss": "^1.20.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-react": "^6.10.0",
    "fs-extra": "^2.1.2",
    "jest-cli": "^19.0.2",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-styleguidist": "^4.6.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1"
  },
  "main": "./index.js",
  "engines": {
    "npm": "^3.0.0"
  },
  "dependencies": {
    "@ox2/colors": "^0.0.2",
    "@ox2/icon": "^3.1.0",
    "@ox2/ui": "^0.21.0",
    "react-select": "^1.0.0-rc.2",
    "styled-components": "^1.4.4"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "jest": {
    "roots": [
      "./src"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
